{
  "name": "parcelmsg",
  "version": "0.0.1",
  "description": "Messenger simple",
  "source": "src/index.html",
  "scripts": {
    "test": "parcel src/index.html --no-source-maps --target default --dist-dir ./build",
    "build": "parcel build src/index.html --no-source-maps --target default --dist-dir ./build",
    "dev": "parcel src/index.html --no-source-maps --target default --dist-dir ./build --no-cache --port 8000 --open",
    "watch": "parcel watch",
    "start": "node ./server.js"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@parcel/transformer-inline-string": "^2.4.1",
    "@parcel/transformer-less": "^2.8.2",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "eslint": "8.32.0",
    "eslint-config-airbnb": "19.0.4",
    "express": "4.18.2",
    "parcel": "^2.4.1",
    "stylelint": "^14.16.1",
    "typescript": "4.9.4"
  },
  "targets": {
    "default": {
      "sourceMap": false,
      "engines": {
        "browsers": ">=50%"
      }
    }
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "dependencies": {
    "handlebars": "4.7.7",
    "less": "4.1.3"
  },
  "include": [
    "src/ts/**/*.ts"
  ]
}
