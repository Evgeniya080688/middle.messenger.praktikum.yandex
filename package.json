{
  "name": "parcelmsg",
  "version": "0.0.1",
  "description": "Messenger simple",
  "source": "src/index.html",
  "scripts": {
    "test": "parcel src/index.html  --no-source-maps --target default --dist-dir ./dist",
    "build": "parcel build src/index.html --no-source-maps --target default --dist-dir ./dist --no-cache",
    "dev": "parcel src/index.html  --no-source-maps --target default --port 8080",
    "watch": "parcel watch",
    "start": "node ./server.js"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@parcel/transformer-inline-string": "2.8.2",
    "@parcel/transformer-less": "2.8.2",
    "express": "4.18.2",
    "parcel": "2.8.2",
    "parcel-transformer-hbs": "1.0.4"
  },
  "targets": {
    "default": {
      "sourceMap": false,
      "engines": {
        "browsers": ">=50%"
      }
    }
  },
  "engines": {
    "node": ">=14.0"
  },
  "dependencies": {
    "handlebars": "4.7.7",
    "less": "4.1.3"
  }
}
